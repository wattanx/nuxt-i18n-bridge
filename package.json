{
  "name": "@wattanx/nuxt-i18n-bridge",
  "version": "0.0.2",
  "repository": "https://github.com/wattanx/nuxt-i18n-bridge",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./module.cjs"
    }
  },
  "main": "./module.cjs",
  "files": [
    "dist",
    "module.cjs"
  ],
  "scripts": {
    "prepack": "unbuild",
    "build:stub": "pnpm prepack --stub",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "pnpm build:stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "pnpm prepack && vitest run",
    "test:webpack": "TEST_WITH_WEBPACK=1 pnpm test",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui"
  },
  "peerDependencies": {
    "@nuxtjs/i18n": "^7.3.1"
  },
  "dependencies": {
    "@babel/parser": "^7.25.9",
    "@intlify/shared": "^9.14.1",
    "@mizchi/sucrase": "^4.1.0",
    "@nuxt/kit": "3.9.3",
    "knitwork": "^1.1.0",
    "pathe": "^1.1.2",
    "ufo": "^1.5.4",
    "vue-i18n": "8.28.2",
    "vue-i18n-bridge": "^9.14.1",
    "vue-i18n-routing": "^1.2.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "0.5.5",
    "@nuxt/schema": "3.9.3",
    "@nuxt/test-utils": "3.10.0",
    "@types/node": "^20.17.0",
    "@vitejs/plugin-vue2": "^2.3.1",
    "@vitest/ui": "1.6.0",
    "changelogen": "^0.5.7",
    "eslint": "8.57.1",
    "jsdom": "^24.1.3",
    "unbuild": "^2.0.0",
    "vitest": "1.6.0",
    "vue": "2.7.16",
    "vue-router": "^3.6.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@8.15.9"
}
